{
  "name": "angulartics-sklik",
  "version": "1.0.0",
  "description": "Sklik plugin for Angulartics",
  "main": "lib/index.js",
  "repository": "git@github.com:chuckyblack/angulartics-sklik.git",
  "author": "Jarom√≠r Pufler <jaromir.pufler@gmail.com>",
  "license": "MIT",
  "keywords": [
    "angularjs",
    "angulartics",
    "analytics",
    "sklik",
    "angulartics-plugin"
  ],
  "scripts": {
    "clean": "rm -rf dist",
    "prebuild": "npm run clean",
